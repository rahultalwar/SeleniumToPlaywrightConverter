{
  "version": 2,
  "routes": [
    {
      "src": "/convert",
      "dest": "/api/convert.py"
    },
    {
      "src": "/models",
      "dest": "/api/models.py"
    },
    {
      "src": "/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "K2_API_URL": "https://api.moonshot.cn/v1/chat/completions",
    "K2_MODEL": "moonshot-v1-8k",
    "DEMO_MODE": "true"
  }
}
